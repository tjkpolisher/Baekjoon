    "# 2435: 기상청 인턴 신현수\n",
    "N, K = map(int, input().split())\n",
    "seq = list(map(int, input().split()))\n",
    "seq_ans = []\n",
    "for i in range(N - K + 1):\n",
    "    seq_ans.append(sum(seq[i:i + K]))\n",
    "print(max(seq_ans))"
